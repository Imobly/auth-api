# Supabase Production Database - TRANSACTION MODE (porta 6543)
# Obtenha no Supabase Dashboard: Settings → Database → Connection String (Transaction)
DATABASE_URL=postgresql://postgres.[PROJECT-ID]:[PASSWORD]@aws-0-sa-east-1.pooler.supabase.com:6543/postgres
DB_SCHEMA=public

# Security - GERAR NOVO SECRET_KEY EM PRODUÇÃO
# Para gerar: openssl rand -hex 32
SECRET_KEY=GERAR_NOVO_SECRET_KEY_AQUI
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# CORS - adicionar domínio de produção
CORS_ORIGINS=https://seu-frontend.onrender.com,https://seu-dominio.com

# Server
HOST=0.0.0.0
PORT=8000
DEBUG=false
